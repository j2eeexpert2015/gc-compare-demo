{
  "annotations": {
    "list": []
  },
  "title": "ZGC: Generational vs Non-Generational Comparison",
  "uid": "zgc-gen-vs-nongen",
  "version": 1,
  "timezone": "browser",
  "schemaVersion": 38,
  "refresh": "5s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "tags": ["zgc", "comparison", "generational"],
  "panels": [
    {
      "title": "GC Pause Time (μs) - Lower is Better",
      "description": "Generational ZGC should have better pause times due to separate young/old generation handling - shown in microseconds for precision",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "max(jvm_gc_pause_seconds_max{job=\"zgc-app\"}) * 1000000",
          "legendFormat": "Generational ZGC - Max Pause",
          "refId": "A"
        },
        {
          "expr": "max(jvm_gc_pause_seconds_max{job=\"zgc-nongen-app\"}) * 1000000",
          "legendFormat": "ZGC - Max Pause",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "µs",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10,
            "pointSize": 5,
            "showPoints": "auto"
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Generational ZGC - Max Pause" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "ZGC - Max Pause" },
            "properties": [{ "id": "color", "value": { "fixedColor": "blue", "mode": "fixed" } }]
          }
        ]
      }
    },
    {
      "title": "GC Pause Count (Total)",
      "description": "Number of GC pauses - both should be sub-millisecond",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "sum(jvm_gc_pause_seconds_count{job=\"zgc-app\"})",
          "legendFormat": "Generational ZGC - Total Pauses",
          "refId": "A"
        },
        {
          "expr": "sum(jvm_gc_pause_seconds_count{job=\"zgc-nongen-app\"})",
          "legendFormat": "ZGC - Total Pauses",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Generational ZGC - Total Pauses" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "ZGC - Total Pauses" },
            "properties": [{ "id": "color", "value": { "fixedColor": "blue", "mode": "fixed" } }]
          }
        ]
      }
    },
    {
      "title": "Heap Memory Used (MB)",
      "description": "Memory consumption pattern - Gen ZGC should handle young objects more efficiently",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "sum(jvm_memory_used_bytes{job=\"zgc-app\", area=\"heap\"}) / 1024 / 1024",
          "legendFormat": "Generational ZGC - Heap Used",
          "refId": "A"
        },
        {
          "expr": "sum(jvm_memory_used_bytes{job=\"zgc-nongen-app\", area=\"heap\"}) / 1024 / 1024",
          "legendFormat": "ZGC - Heap Used",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "decmbytes",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Generational ZGC - Heap Used" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "ZGC - Heap Used" },
            "properties": [{ "id": "color", "value": { "fixedColor": "blue", "mode": "fixed" } }]
          }
        ]
      }
    },
    {
      "title": "Process CPU Usage",
      "description": "CPU overhead - Gen ZGC may use slightly more CPU for generation tracking",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "targets": [
        {
          "expr": "avg(process_cpu_usage{job=\"zgc-app\"})",
