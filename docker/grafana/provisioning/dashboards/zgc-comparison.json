          "refId": "B"
        },
        {
          "expr": "sum(jvm_gc_pause_seconds_count{job=\"zgc-app\"})",
          "legendFormat": "ZGC Gen Total Pauses",
          "refId": "C"
        },
        {
          "expr": "sum(jvm_gc_pause_seconds_count{job=\"zgc-nongen-app\"})",
          "legendFormat": "ZGC NonGen Total Pauses",
          "refId": "D"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "ZGC Gen Max Pause (ms)" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "ZGC NonGen Max Pause (ms)" },
            "properties": [{ "id": "color", "value": { "fixedColor": "blue", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "ZGC Gen Total Pauses" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "ZGC NonGen Total Pauses" },
