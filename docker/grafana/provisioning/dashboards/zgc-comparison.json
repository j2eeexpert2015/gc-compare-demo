    {
      "title": "Generational ZGC Cycles by Reason",
      "description": "Why Generational ZGCerational cycles are triggered",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 30 },
      "targets": [
        {
          "expr": "sum(rate(jvm_gc_pause_seconds_count{job=\"zgc-app\", action=\"end of major GC\"}[1m]))",
          "legendFormat": "Major GC Cycles",
          "refId": "A"
        },
        {
          "expr": "sum(rate(jvm_gc_pause_seconds_count{job=\"zgc-app\", cause=\"Allocation Rate\"}[1m]))",
          "legendFormat": "Allocation Rate",
          "refId": "B"
        },
        {
          "expr": "sum(rate(jvm_gc_pause_seconds_count{job=\"zgc-app\", cause=\"Proactive\"}[1m]))",
          "legendFormat": "Proactive",
          "refId": "C"
        },
        {
          "expr": "sum(rate(jvm_gc_pause_seconds_count{job=\"zgc-app\", cause=\"Warmup\"}[1m]))",
          "legendFormat": "Warmup",
          "refId": "D"
        },
        {
          "expr": "sum(rate(jvm_gc_pause_seconds_count{job=\"zgc-app\", cause=\"Allocation Stall\"}[1m]))",
          "legendFormat": "⚠️ Allocation Stall",
          "refId": "E"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10,
            "stacking": {
              "mode": "normal"
            }
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "⚠️ Allocation Stall" },
            "properties": [
              { "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Allocation Rate" },
            "properties": [
              { "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Proactive" },
            "properties": [
              { "id": "color", "value": { "fixedColor": "light-green", "mode": "fixed" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Warmup" },
            "properties": [
              { "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }
