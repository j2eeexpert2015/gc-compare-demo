{
  "id": null,
  "uid": "gc-core-compare",
  "title": "GC Core Comparison – G1 vs Gen ZGC vs ZGC",
  "tags": ["gc", "zgc", "g1gc"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "style": "dark",
  "editable": true,
  "graphTooltip": 1,
  "refresh": "5s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"]
  },
  "templating": {
    "list": []
  },
  "annotations": {
    "list": []
  },
  "links": [],
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Request Rate (RPS) – /api/better/natural/*",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "req/s",
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (gc_type) (rate(http_server_requests_seconds_count{uri=~\"/api/better/natural.*\"}[1m]))",
          "legendFormat": "{{gc_type}}"
        }
      ]
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "P99 Request Latency (ms) – /api/better/natural/*",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.99, sum by (le, gc_type) (rate(http_server_requests_seconds_bucket{uri=~\"/api/better/natural.*\"}[1m]))) * 1000",
          "legendFormat": "{{gc_type}}"
        }
      ]
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "GC Max Pause (ms) – by GC mode",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "max by (gc_type) (jvm_gc_pause_seconds_max) * 1000",
          "legendFormat": "{{gc_type}}"
        }
      ]
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "GC Overhead (% of time in GC)",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "100 * sum by (gc_type) (rate(jvm_gc_pause_seconds_sum[1m]))",
          "legendFormat": "{{gc_type}}"
        }
      ]
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Heap Used (GB) – by GC mode",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 16
      },
      "fieldConfig": {
        "defaults": {
          "unit": "gbytes",
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (gc_type) (jvm_memory_used_bytes{area=\"heap\"}) / (1024 * 1024 * 1024)",
          "legendFormat": "{{gc_type}}"
        }
      ]
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Heap Overhead (Committed – Used) (GB)",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 24
      },
      "fieldConfig": {
        "defaults": {
          "unit": "gbytes",
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "(sum by (gc_type) (jvm_memory_committed_bytes{area=\"heap\"}) - sum by (gc_type) (jvm_memory_used_bytes{area=\"heap\"})) / (1024 * 1024 * 1024)",
          "legendFormat": "{{gc_type}}"
        }
      ]
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Process CPU Usage (%) – by GC mode",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 24
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "avg by (gc_type) (process_cpu_usage) * 100",
          "legendFormat": "{{gc_type}}"
        }
      ]
    }
  ]
}
